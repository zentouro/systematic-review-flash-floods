@inproceedings{ ISI:000168052200005,
Author = {Montz, BE and Evans, TA},
Editor = {{Gruntfest, E and Handmer, J}},
Title = {{GIS and social vulnerability analysis}},
Booktitle = {{COPING WITH FLASH FLOODS}},
