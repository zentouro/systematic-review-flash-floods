@article{ ISI:000415973500013,
Author = {Huebl, Johannes},
Title = {{Flashflood Simbach 2016: Documentation and Analysis}},
Journal = {{WASSERWIRTSCHAFT}},
Year = {{2017}},
Volume = {{107}},
Number = {{11}},
Pages = {{61-64}},
Abstract = {{On June 1st, 2016, in Simbach am Inn, Germany a flash flood occured. The
   event originated after heavy rainfall in the river Simbach and caused
   five fatalities and severe damages on residential, non-residential
   properties and infrastructure. During this event the failure of a road
   dam and a lateral dyke lead to a sudden increase of the discharge within
   the town. On behalf of the Bavarian Environment Agency this flash flood
   was investigated in order to evaluate the impact of these failures. It
   may be evidenced that the flooded area with or without the failures is
   more or less the same, but the flow depths differ about half a meter
   within the town.}},
