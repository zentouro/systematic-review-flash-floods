@article{ ISI:000400522200008,
Author = {Bass, Benjamin and Juan, Andrew and Gori, Avantika and Fang, Zheng and
   Bedient, Philip},
Title = {{2015 Memorial Day Flood Impacts for Changing Watershed Conditions in
   Houston}},
