Author = {Goron, Nicoleta Daniela},
Book-Group-Author = {{Nicolae Balcescu Land Forces Acad}},
Title = {{RISK ANALYSIS: MAJOR FLASH FLOODS IN THE UPPER RIVER BASIN OF MURES.
   STANCENI}},
Booktitle = {{17TH INTERNATIONAL CONFERENCE - THE KNOWLEDGE-BASED ORGANIZATION:
   APPLIED TECHNICAL SCIENCES AND ADVANCED MILITARY TECHNOLOGIES,
   CONFERENCE PROCEEDING 3}},
Series = {{Knowledge Based Organization International Conference}},
