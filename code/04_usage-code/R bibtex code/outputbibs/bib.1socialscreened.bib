@article{ ISI:000368343500006,
Author = {Morss, Rebecca E. and Demuth, Julie L. and Bostrom, Ann and Lazo,
   Jeffrey K. and Lazrus, Heather},
Title = {{Flash Flood Risks and Warning Decisions: A Mental Models Study of
   Forecasters, Public Officials, and Media Broadcasters in Boulder,
   Colorado}},
