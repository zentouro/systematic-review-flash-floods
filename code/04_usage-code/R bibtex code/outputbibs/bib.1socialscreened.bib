@article{ ISI:000284916200005,
Author = {Czigany, Szabolcs and Pirkhoffer, Ervin and Nagyvaradi, Laszlo and
   Szebenyi, Anita and Vass, Peter and Geresdi, Istvan},
Title = {{Potential areas of flood impoundment in human environments, Bukkosd
   Valley, SW Hungary}},
Journal = {{GEOMORPHOLOGIE-RELIEF PROCESSUS ENVIRONNEMENT}},
Year = {{2010}},
Number = {{3}},
Pages = {{301-310}},
Month = {{JUL-SEP}},
Abstract = {{As a consequence of the process of suburbanisation and the decrease of
   natural landscapes and permeable soil surfaces rainfall triggered,
   increased surface runoff and so-called urban floods are often causes
   significant loss in human properties and may pose a threat to human
   lives Such processes are studied for the Bukkosd Stream catchment, SW
   Hungary using the topographical, surface water and land use databases
   The villages of Bukkosd and Hetvehely as part of the Pecs agglomeration
   have been impounded by floodwaters several times since regular
   observations began To mitigate future economic losses the objective of
   the present study was to carry out a rapid method (vertical distance
   based inundation mapping, VDBIM) to delineate the extent of impounded
   areas at 1 m 5 m and 10 m above the actual riverbed Total inundated
   areas in the floodplain of the Bukkosd Stream cover 102 ha, 151 ha and
   178 ha at water depths of 1 m 5 m and 10 m, respectively Due to the
   narrow valleys in the headwater section of the catchment the combined
   area of the flooded valleys was only 1 75 larger at 10-m water depth
   than at 1-m impoundment Impounded areas include residential areas
   transport routes (primarily the railway) industrial estates, and
   agricultural and food processing facilities Flash floods pose a
   significant threat to railway embankments as this strategic importance
   railroad connects Pecs with the national capital}},
