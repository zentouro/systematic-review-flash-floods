@article{ ISI:000357732800003,
Author = {Okada, Tetsuya and Haynes, Katharine and Bird, Deanne and van den
   Honert, Robin and King, David},
Title = {{Recovery and resettlement following the 2011 flash flooding in the
   Lockyer Valley}},
