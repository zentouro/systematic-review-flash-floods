@article{ ISI:000347161900049,
Author = {Douvinet, Johnny and Van De Wiel, Marco J. and Delahaye, Daniel and
   Cossart, Etienne},
Title = {{A flash flood hazard assessment in dry valleys (northern France) by
   cellular automata modelling}},
Journal = {{NATURAL HAZARDS}},
