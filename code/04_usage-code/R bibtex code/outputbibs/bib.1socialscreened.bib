Title = {{Management of Risk from Flash Floods in The South-Eastern Slovakia}},
Booktitle = {{WORLD MULTIDISCIPLINARY CIVIL ENGINEERING-ARCHITECTURE-URBAN PLANNING
   SYMPOSIUM 2016, WMCAUS 2016}},
