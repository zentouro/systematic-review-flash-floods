@article{ ISI:000221663500010,
Author = {Andrieu, H and Browne, O and Laplace, D},
Title = {{Are Urban floods flash floods?}},
Journal = {{HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU}},
Year = {{2004}},
Number = {{2}},
Pages = {{89-95}},
Abstract = {{Urban hydrology has developed together with the urbanization, and has
   focused on the hydraulic design of sewer systems. Intense rain events,
   for instance Nimes in 1988, have shown the major bed of rainwater sewer
   network exists and is formed by the street network. Expressions such as
   ``urban floods{''} or ``floods of urban runoff{''} illustrate this
   finding which has been followed by reflections addressing the problem of
   intense rain events on urbanized areas. Can we consider urban floods as
   flash floods? The response is not straightforward even if urban
   catchments display features: small surface areas, short response times
   making them sensitive to intense rain events of small duration. This
   paper attempts to contribute to the reflections on this subject through
   two case studies. Seine-Saint-Denis and Marseille.}},
