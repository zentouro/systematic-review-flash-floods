@article{ ISI:000301946700007,
Author = {Sayama, Takahiro and Ozawa, Go and Kawakami, Takahiro and Nabesaka,
   Seishi and Fukami, Kazuhiko},
Title = {{Rainfall-runoff-inundation analysis of the 2010 Pakistan flood in the
   Kabul River basin}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2012}},
Volume = {{57}},
Number = {{2}},
Pages = {{298-312}},
Abstract = {{Pakistan has suffered a devastating flood disaster in 2010. In the Kabul
   River basin (92 605 km(2)), large-scale riverine and flash floods caused
   destructive damage with more than 1100 casualties. This study analysed
   rainfall-runoff and inundation in the Kabul River basin with a newly
   developed model that simulates the processes of rainfall-runoff and
   inundation simultaneously based on two-dimensional diffusion wave
   equations. The simulation results showed a good agreement with an
   inundation map produced based on MODIS for large-scale riverine
   flooding. In addition, the simulation identified flash flood-affected
   areas, which were confirmed to be severely damaged based on a housing
   damage distribution map. Since the model is designed to be used even
   immediately after a disaster, it can be a useful tool for analysing
   large-scale flooding and to provide supplemental information to agencies
   for relief operations.}},
